generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model GasStation {
  id        Int    @id @default(autoincrement())
  name      String
  email     String @unique
  password  String
  latitude  Float
  longitude Float
  gasPrice  Float  @map("gas_price")

  @@map("gas_stations")
}
